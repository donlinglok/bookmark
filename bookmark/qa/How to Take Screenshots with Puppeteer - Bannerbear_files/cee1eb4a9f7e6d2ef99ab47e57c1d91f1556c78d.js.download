	function toggle_pricing() {
		$('.toggle_switch, .pricingtable').removeClass('monthly annual')
		$('.toggle_switch .monthly span, .toggle_switch .annual span').removeClass('selected')
		if ($('.toggle_switch input').prop("checked") == true) {
